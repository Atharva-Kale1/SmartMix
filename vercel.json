{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "installCommand": "pip install -r requirements.txt && npm install",
  "runtimes": [
    {
      "name": "node",
      "version": "20.x"
    },
    {
      "name": "python",
      "version": "3.12"
    }
  ]
}
