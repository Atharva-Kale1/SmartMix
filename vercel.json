{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/recommendation_engine.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/recommend",
      "dest": "api/recommendation_engine.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "installCommand": "npm install"
}
